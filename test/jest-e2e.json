{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "./",
  "modulePaths": [
    "<rootDir>"
  ],
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@common/(.*)": "<rootDir>/../libs/common/src/$1",
    "@common": "<rootDir>/../libs/common/src",
    "@ezreal/(.*)": "<rootDir>/../libs/ezreal/src/$1",
    "@ezreal": "<rootDir>/../libs/ezreal/src"
  }
}